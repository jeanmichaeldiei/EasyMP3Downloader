{
  "manifest_version": 2,
  "name": "YTtoMP3",
  "version": "0.1",
  
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["jquery-3.2.1.min.js", "content.js"]
  }
  ],
  
  "browser_action": {
  "default_icon": "icon.png"
  },

  "background": {
  "scripts": ["jquery-3.2.1.min.js", "background.js"],

  "permissions": [
    "http://*/"
  ]
}
  
}
